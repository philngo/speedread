{
    "manifest_version": 2,

    "name": "Speed Reader",
    "description": "For speed reading",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
